<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">

 <object name="document_actions" meta_type="CMF Action Category">
  <object name="print" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Print this</property>
   <property name="url_expr">string:javascript:this.print();</property>
   <property name="visible">True</property>
  </object>
  <object name="sendto" meta_type="CMF Action">
   <property name="title">Invia</property>
   <property name="url_expr">string:$object_url/sendto_form</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions"/>
   <property name="visible">True</property>
  </object>
 </object>

 <object name="site_actions" meta_type="CMF Action Category">
  <object name="amm-trasp" meta_type="CMF Action">
   <property name="title">Amministrazione Trasparente</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/trasparenza</property>
   <property name="visible">True</property>
  </object>
  <object name="albo" meta_type="CMF Action">
   <property name="title">Albo Camerale</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/albo-camerale</property>
   <property name="visible">True</property>
  </object>
  <object name="chi-siamo" meta_type="CMF Action">
   <property name="title">Chi siamo</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/camera-commercio/chi-siamo</property>
   <property name="visible">True</property>
  </object>
  <object name="privacy" meta_type="CMF Action">
   <property name="title">Privacy Policy</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/privacy</property>
   <property name="visible">True</property>
  </object>
  <object name="sitemap" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Site Map</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/sitemap</property>
   <property name="visible">True</property>
  </object>
  <object name="accessibility" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Accessibility</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/accessibility-info</property>
   <property name="visible">True</property>
  </object>
  <object name="note-legali" meta_type="CMF Action">
   <property name="title">Note legali</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/note-legali</property>
   <property name="visible">True</property>
  </object>
  <object name="bandi" meta_type="CMF Action">
   <property name="title">Bandi di gara e di concorso</property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/gare-e-contratti</property>
   <property name="visible">True</property>
  </object>
  <object name="contact" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Contact</property>
   <property name="visible">False</property>
  </object>
  <object name="plone_setup" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Site Setup</property>
   <property name="visible">False</property>
  </object>
 </object>
 
</object>